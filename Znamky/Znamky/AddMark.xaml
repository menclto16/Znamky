<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Znamky.AddMark">
    <ContentPage.Content>
        <StackLayout Orientation="Vertical">
            <Entry x:Name="Subject" Completed="InsertMark"></Entry>
            <Entry x:Name="Mark" Completed="InsertMark"></Entry>
            <Entry x:Name="Weight" Completed="InsertMark"></Entry>
            <Entry x:Name="Comment" Completed="InsertMark"></Entry>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>